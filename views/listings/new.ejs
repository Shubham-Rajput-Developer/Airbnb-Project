<% layout("/layouts/boilerPlate.ejs") %> 
<body>
    <h2>Create a new Listing</h2>
    <form  method="POST" action="/listings">
        Enter title: <br>
        <input type="text" name="listing[title]" placeholder="Enter title"><br>
        <br>Enter description: <br>
        <textarea name="listing[description]" placeholder="Enter description"></textarea><br>
        <br>Enter image link: <br>
        <input type="text" name="listing[image]" placeholder="Enter image Link"><br>
        <br>Enter price: <br>
        <input type="number" name="listing[price]" placeholder="Enter price"><br>
        <br>Enter location: <br>
        <input type="text" name="listing[location]" placeholder="Enter location"><br>
        <br>Enter country name: <br>
        <input type="text" name="listing[country]" placeholder="Enter country name"><br>
        <br><button>Add</button>
    </form>
</body>